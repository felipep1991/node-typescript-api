    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-pessoal:felipep1991/node-typescript-api.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "felipe",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "module-alias": "^2.2.2"
  }
